<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox id="searchBar" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ViewHandler">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Pets">
               <items>
                  <MenuItem fx:id="petsMenu" mnemonicParsing="false" text="Pet" />
               </items></Menu>
        <Menu mnemonicParsing="false" text="Customers">
               <items>
                  <MenuItem fx:id="customersMenu" mnemonicParsing="false" text="Customers" />
               </items></Menu>
        <Menu mnemonicParsing="false" text="Bookings">
               <items>
                  <MenuItem fx:id="bookingsListMenu" mnemonicParsing="false" text="View" />
                  <MenuItem fx:id="bookingsSettingsMenu" mnemonicParsing="false" text="Settings" onAction="#switchScene"/>
               </items></Menu>
            <Menu mnemonicParsing="false" text="Sales">
               <items>
                  <MenuItem fx:id="salesMenu" mnemonicParsing="false" text="Sales" />
               </items></Menu>
      </menus>
    </MenuBar>
  </children>
</VBox>
