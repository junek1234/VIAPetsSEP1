<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox id="searchBar" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.FishViewController">
  <children>
      <HBox maxHeight="-Infinity" prefHeight="18.0" prefWidth="640.0">
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin>
         <children>
            <Label prefHeight="34.0" prefWidth="60.0" text="Name">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="petNameEditTextField" prefHeight="0.0" prefWidth="118.0" />
            <Label prefHeight="34.0" prefWidth="60.0" text="Color">
               <HBox.margin>
                  <Insets left="75.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="petColorEditTextField" prefHeight="0.0" prefWidth="118.0" />
         </children>
      </HBox>
      <HBox maxHeight="-Infinity" prefHeight="18.0" prefWidth="640.0">
         <children>
            <Label prefHeight="34.0" prefWidth="60.0" text="Age">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="petAgeEditTextField" prefHeight="0.0" prefWidth="118.0" />
            <Label prefHeight="34.0" prefWidth="60.0" text="Gender">
               <HBox.margin>
                  <Insets left="70.0" />
               </HBox.margin>
            </Label>
            <RadioButton fx:id="petGenderMaleEditRadioButton" mnemonicParsing="false" text="Male" />
            <RadioButton fx:id="petGenderFemaleEditRadioButton" mnemonicParsing="false" text="Female">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </RadioButton>
         </children>
         <VBox.margin>
            <Insets top="25.0" />
         </VBox.margin>
      </HBox>
      <HBox maxHeight="-Infinity" prefHeight="18.0" prefWidth="640.0">
         <children>
            <Label prefHeight="34.0" prefWidth="60.0" text="Location">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Label>
            <RadioButton fx:id="petLocationShopEditRadioButton" mnemonicParsing="false" text="Shop" />
            <RadioButton fx:id="petLocationKennelEditRadioButton" mnemonicParsing="false" text="Kennel">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </RadioButton>
            <Label prefHeight="34.0" prefWidth="60.0" text="Price">
               <HBox.margin>
                  <Insets left="70.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="petPriceEditTextField" prefHeight="0.0" prefWidth="118.0" />
         </children>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </HBox>
      <HBox maxHeight="-Infinity" prefHeight="18.0" prefWidth="640.0">
         <children>
            <Label prefHeight="18.0" prefWidth="65.0" text="Status">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Label>
            <RadioButton fx:id="petStatusSoldEditRadioButton" mnemonicParsing="false" prefHeight="18.0" prefWidth="74.0" text="Sold" />
            <RadioButton fx:id="petStatusNotSoldEditRadioButton" mnemonicParsing="false" prefHeight="18.0" prefWidth="98.0" text="Not sold">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </RadioButton>
            <RadioButton fx:id="petStatusNotFromViaEditRadioButton" mnemonicParsing="false" prefHeight="18.0" prefWidth="187.0" text="Not from VIAPets" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </HBox>
      <HBox maxHeight="-Infinity" prefHeight="18.0" prefWidth="640.0">
         <children>
            <Label prefHeight="34.0" prefWidth="60.0" text="Salt water">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Label>
            <RadioButton fx:id="petSaltWaterYesEditRadioButton" mnemonicParsing="false" text="Yes" />
            <RadioButton fx:id="petSaltWaterNoEditRadioButton" mnemonicParsing="false" text="No">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </RadioButton>
            <Label prefHeight="26.0" prefWidth="80.0" text="Predator">
               <HBox.margin>
                  <Insets left="100.0" />
               </HBox.margin>
            </Label>
            <RadioButton fx:id="petPredatorYesEditRadioButton" mnemonicParsing="false" text="Yes" />
            <RadioButton fx:id="petPredatorNoEditRadioButton" mnemonicParsing="false" text="No">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </RadioButton>
         </children>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </HBox>
      <HBox maxHeight="-Infinity" prefHeight="88.0" prefWidth="640.0">
         <children>
            <Label prefHeight="34.0" prefWidth="60.0" text="Species">
               <HBox.margin>
                  <Insets left="50.0" top="20.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="petSpeciesEditTextField" prefHeight="0.0" prefWidth="118.0">
               <HBox.margin>
                  <Insets top="20.0" />
               </HBox.margin></TextField>
            <VBox alignment="CENTER" prefHeight="90.0" prefWidth="329.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="63.0" prefWidth="273.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="25.0" prefWidth="80.0" text="Comment" textAlignment="CENTER">
                           <HBox.margin>
                              <Insets left="105.0" />
                           </HBox.margin>
                        </Label>
                        <TextArea fx:id="petCommentEditTextField" prefHeight="78.0" prefWidth="158.0">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </TextArea>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <Button fx:id="petSaveButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#saveEditFish" text="Save">
         <VBox.margin>
            <Insets bottom="10.0" left="275.0" top="20.0" />
         </VBox.margin>
      </Button>
  </children>
</VBox>
